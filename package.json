    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.3",
    "@types/node": "^18.19.29",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "vite-plugin-biome": "^1.0.9",
    "vitest": "^1.4.0"
  },
  "packageManager": "pnpm@8.15.6"
}
